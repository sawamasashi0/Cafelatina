<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mac_coco.cafelatina">
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="CafeLatina 伝票アプリ"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cafelatina.my.cafelatina.EntryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cafelatina.my.cafelatina.OrderActivity" />
        <activity android:name="cafelatina.my.cafelatina.CommandActivity" >
        <intent-filter>
            <action android:name="cafelatina.my.cafelatina.COMMAND" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>
        <activity android:name="cafelatina.my.cafelatina.Popup"/>

        <activity android:name="cafelatina.my.cafelatina.EndActivity"/>
    </application>


</manifest>